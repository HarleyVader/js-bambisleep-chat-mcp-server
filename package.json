{
    "name": "js-bambisleep-chat-mcp-server",
    "version": "1.0.0",
    "description": "MCP Server & Agent Docking System",
    "main": "server/src/server.js",
    "private": true,
    "workspaces": [
        "server",
        "agent",
        "shared"
    ],
    "scripts": {
        "build": "npm install && npm install --workspace=server && npm install --workspace=agent && npm install --workspace=shared && npm run install:agent --workspace=agent && npm run build --workspace=agent && npx cpy \"agent/js-bambisleep-chat-agent-dr-girlfriend/dist/**/*\" \"server/public/agent-app/\"",
        "build-agents": "node scripts/build-agents.mjs",
        "start": "cd server && npm start",
        "postinstall": "echo 'Installation complete'"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "github",
        "oauth",
        "docker"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "cpy-cli": "^6.0.0"
    }
}
