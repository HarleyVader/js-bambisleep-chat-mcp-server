{
    "name": "js-bambisleep-chat-mcp-server",
    "version": "1.0.0",
    "description": "MCP Server & Agent Docking System",
    "main": "index.js",
    "private": true,
    "workspaces": [
        "server",
        "agent",
        "shared",
        "patreon"
    ],
    "scripts": {
        "build": "npm install --workspaces && npm run build --workspaces && npx cpy \"agent/js-bambisleep-chat-agent-dr-girlfriend/dist/**/*\" \"server/public/agent-app/\" && npx cpy \"patreon/dist/**/*\" \"server/public/patreon/\"",
        "start": "cd server && npm start",
        "install-all": "npm install --workspaces --include-workspace-root",
        "postinstall": "echo 'Root package installed successfully'"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "github",
        "oauth",
        "docker"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "cpy-cli": "^6.0.0"
    }
}
